<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="HotelTransylvaniaMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HotelTransylvaniaMaui"
    Shell.FlyoutBehavior="Flyout"
    FlyoutBackgroundColor="DarkSlateGray">
    <FlyoutItem
        Title="Hotel Transylvania Main Page"
        Icon="zombiedoor.png">
        <ShellContent
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage">
        </ShellContent>
    </FlyoutItem>
</Shell>
